{
    "database": {
        "host": "localhost",
        "port": 5432,
        "dbname": "statistics",
        "password": "spark",
        "schema": "stat_compiled",
        "insert_count": 100
    },
    "logger": {
        "version": 1,
        "disable_existing_loggers": "False",
        "formatters": {
            "default": {
                "format": "[%(asctime)s] [%(levelname)5s] [%(process)5s] [%(name)10s] %(message)s"
            }
        },
        "handlers": {
            "default": {
                "level": "DEBUG",
                "class": "logging.StreamHandler",
                "formatter": "default"
            }
        },
        "loggers": {
            "": {
                "handlers": ["default"],
                "level": "DEBUG",
                "propagate": "True"
            }
        }
    }
}